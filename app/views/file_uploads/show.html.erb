<p id="notice"><%= notice %></p>

<p>
  <strong>File name:</strong>
  <%= @file_upload.file_name %>
</p>

<p>
  <strong>User:</strong>
  <%= @file_upload.user_id %>
</p>

<p>
  <strong>File path:</strong>
  <%= @file_upload.file_path %>
</p>

<p>
  <strong>Mime type:</strong>
  <%= @file_upload.mime_type %>
</p>

<p>
  <strong>File Hash:</strong>
  <%= @file_upload.file_hash %>
</p>

<p>
  <strong>Size:</strong>
  <%= @file_upload.size %>
</p>

<p>
  <%= link_to "Download", download_path(@file_upload.file_hash), class: 'btn btn-success' %>
</p>
<%= link_to 'Back', file_uploads_path %>
